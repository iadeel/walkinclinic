    <script>
        $(function () {
            if (window.location.href.indexOf('ReferralForm') !== -1)
                $('ul.nav li a[href$="ReferralForm"]').parent().addClass("active");
            else if (window.location.href.indexOf('CardiologyClinic') !== -1)
                $('ul.nav li a[href$="CardiologyClinic"]').parent().addClass("active");
            else if (window.location.href.indexOf('WomenHealthClinic') !== -1)
                $('ul.nav li a[href$="WomenHealthClinic"]').parent().addClass("active");
            else if (window.location.href.indexOf('ClinicResearchAndTrials') !== -1)
                $('ul.nav li a[href$="ClinicResearchAndTrials"]').parent().addClass("active");
            else if (window.location.href.indexOf('Careers') !== -1)
                $('ul.nav li a[href$="Careers"]').parent().addClass("active");
            else if (window.location.href.indexOf('ContactUs') !== -1)
                $('ul.nav li a[href$="ContactUs"]').parent().addClass("active");
            else 
                $('ul.nav li a[href$="Home"]').parent().addClass("active");
        });
    </script>

